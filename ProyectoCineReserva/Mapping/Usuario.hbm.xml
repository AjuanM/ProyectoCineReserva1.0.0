<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="ProyectoCineReserva"
                   namespace="ProyectoCineReserva.Models"
                   default-lazy="false">
  <class name="Usuario" table="Usuario">
    <id name="idUsuario" access="property" column="idUsuario">
    <generator class="native" />
    </id>
    <property name="Nombre" column="Nombre" />
    <property name="Apellido" column="Apellido"/>
    <property name="Direccion" column="Direccion"/>
    <property name="Telefono" column="Telefono" />
    <property name="Dni" column="Dni"/>
    <property name="userName" column="userName"/>
    <property name="Clave" column="Clave" />
    <property name="TipoUsuario" column="TipoUsuario"/>
  
  </class>
</hibernate-mapping>